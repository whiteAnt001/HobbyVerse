<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.springboot.hobbyverse.mapper.CategoryMapper">
	<!-- 모임 아이디로 카트 고르는 매퍼(신청된 모임 가져오기) -->
	<select id="selectCart" parameterType="String" resultType="com.springboot.hobbyverse.model.Cart">
		select code, num from cart where id=#{id}
	</select>
</mapper>